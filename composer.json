{
    "name": "venyii/hipchat-commander",
    "description": "A PHP Hipchat application",
    "license": "MIT",
    "type": "project",
    "autoload": {
      "psr-4": {
          "Venyii\\HipChatCommander\\": "src/HipChatCommander",
          "Venyii\\HipChatCommander\\Package\\": "pkg"
      }
    },
    "autoload-dev": {
      "psr-4": {
          "Venyii\\HipChatCommander\\Test\\": "tests/HipChatCommander/Test",
          "Venyii\\HipChatCommander\\Test\\Package\\": "tests/pkg"
      }
    },
    "require": {
        "php": ">=5.6",
        "silex/silex": "~1.3",
        "cilex/console-service-provider": "~1.0.0",
        "symfony/console": "~2.8",
        "symfony/filesystem": "~2.8",
        "symfony/yaml": "~2.8",
        "guzzlehttp/guzzle": "~5.0",
        "beberlei/assert": "~2.3",
        "doctrine/cache": "~1.4",
        "monolog/monolog": "~1.15"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.4",
        "phpmd/phpmd" : "@stable",
        "symfony/browser-kit": "~2.8",
        "friendsofphp/php-cs-fixer": "~1.11",
        "mikey179/vfsStream": "*@stable"
    }
}
