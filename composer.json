{
    "name": "venyii/hipchat-commander",
    "description": "A PHP Hipchat application",
    "license": "MIT",
    "type": "project",
    "autoload": {
      "psr-4": {
          "Venyii\\HipChatCommander\\": "src/HipChatCommander",
          "Venyii\\HipChatCommander\\Package\\": "pkg"
      }
    },
    "autoload-dev": {
      "psr-4": { "Venyii\\HipChatCommander\\Test\\": "tests" }
    },
    "require": {
        "php": ">=5.5",
        "silex/silex": "~1.2",
        "cilex/console-service-provider": "~1.0.0",
        "symfony/dom-crawler": "~2.6",
        "symfony/css-selector": "~2.6",
        "symfony/console": "~2.6",
        "symfony/filesystem": "~2.6",
        "symfony/yaml": "~2.6",
        "guzzlehttp/guzzle": "~5.0",
        "beberlei/assert": "~2.3",
        "doctrine/cache": "~1.4",
        "monolog/monolog": "~1.13"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "phpmd/phpmd" : "@stable",
        "symfony/browser-kit": "~2.6",
        "fabpot/php-cs-fixer": "2.0.*@dev",
        "mikey179/vfsStream": "*@stable"
    }
}
